RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

RewriteRule ^ai-agentic$ ai-agentic.php [NC,QSA]
RewriteRule ^cab-service$ cab-service.php [NC,QSA]
RewriteRule ^social-media$ social-media.php [NC,QSA]
RewriteRule ^emr$ emr.php [NC,QSA]

